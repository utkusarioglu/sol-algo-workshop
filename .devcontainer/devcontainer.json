{
  "name": "sol-algo-workshop.workshops.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "service": "sol-algo-workshop",
  "workspaceFolder": "/utkusarioglu/workshops/sol-algo-workshop",
  "postCreateCommand": "scripts/post-create-command.sh",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#155a6b",
          "activityBar.foreground": "#000000"
        }
      },
      "extensions": [
        "joaompinto.vscode-graphviz",
        "cschleiden.vscode-github-actions",
        "GitHub.vscode-pull-request-github",
        "esbenp.prettier-vscode",
        "searKing.preview-vscode",
        "NomicFoundation.hardhat-solidity",
        "hosho.solidity-debugger"
      ]
    }
  }
}
